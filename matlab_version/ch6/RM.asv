% 迭代均值（期望Ex)的计算公式可以转化为RM问题，从而进行收敛性分析
N = 10000;
x = 5 + rand(N,1)-0.5;

omega(1) = 6;
for i = 2:N
    % alpha = 1/(i^0.6);
    alpha = 1/i;
    % alpha = 0.1;
    % alpha = 0.01;
    omega(i) = omega(i-1) - alpha * (omega(i-1) - x(i-1));

end
plot(omega) %[output:9b76e6f4]
plot(x) %[output:682ba8b8]

%%
%% 分析线性时不变系统：ω(k+1) = (1-α)ω(k) + α*u(k)
%% 线性时不变系统阶跃响应分析
clear; clc; close all;

% 系统参数
alpha_values = [0.1, 1.0, 1.8, 2];  % 不同的α值
N = 50;  % 迭代次数

% 创建图形
figure('Position', [100, 100, 1200, 500]); %[output:825ceb72]

%% 1. 阶跃响应曲线
subplot(1, 2, 1); %[output:825ceb72]
hold on; %[output:825ceb72]
colors = jet(length(alpha_values));

for i = 1:length(alpha_values)
    alpha = alpha_values(i);
    omega = zeros(N, 1);
    
    % 输入：单位阶跃 u(k) = 1
    u = ones(N, 1);
    
    % 迭代计算
    for k = 1:N-1
        omega(k+1) = (1-alpha)*omega(k) + alpha*u(k);
    end
    
    % 绘制响应曲线
    plot(0:N-1, omega, 'LineWidth', 2, ... %[output:825ceb72]
         'Color', colors(i,:), ... %[output:825ceb72]
         'DisplayName', sprintf('α=%.1f', alpha)); %[output:825ceb72]
end

% 图形设置
xlabel('迭代次数 k', 'FontSize', 12); %[output:825ceb72]
ylabel('输出 ω(k)', 'FontSize', 12); %[output:825ceb72]
title('不同α值的阶跃响应', 'FontSize', 14); %[output:825ceb72]
legend('位置', 'best', 'NumColumns', 2); %[output:825ceb72]
grid on; %[output:825ceb72]
xlim([0, N-1]); %[output:825ceb72]
ylim([-0.5, 2.5]); %[output:825ceb72]

% 标记稳态值
yline(1, 'k--', 'LineWidth', 1.5, 'DisplayName', '稳态值=1'); %[output:825ceb72]

%% 2. 理论解析解分析
subplot(1, 2, 2); %[output:825ceb72]
hold on; %[output:825ceb72]

k = 0:N-1;

% 计算并绘制理论解
for i = 1:length(alpha_values)
    alpha = alpha_values(i);
    
    % 理论解：ω(k) = 1 - (1-α)^k
    omega_theoretical = 1 - (1-alpha).^k;
    
    plot(k, omega_theoretical, '--', 'LineWidth', 1.5, ... %[output:825ceb72]
         'Color', colors(i,:), ... %[output:825ceb72]
         'DisplayName', sprintf('α=%.1f 理论', alpha)); %[output:825ceb72]
end

% 标记不同收敛模式区域
text(5, 2.0, '稳定区域 (0<α<2)', 'FontSize', 11, 'BackgroundColor', 'w'); %[output:825ceb72]
text(5, 1.7, '• α<1: 单调上升', 'FontSize', 10, 'BackgroundColor', 'w'); %[output:825ceb72]
text(5, 1.5, '• α=1: 一步到达', 'FontSize', 10, 'BackgroundColor', 'w'); %[output:825ceb72]
text(5, 1.3, '• 1<α<2: 振荡收敛', 'FontSize', 10, 'BackgroundColor', 'w'); %[output:825ceb72]
text(5, 1.1, '• α>2: 发散振荡', 'FontSize', 10, 'BackgroundColor', 'w'); %[output:825ceb72]

xlabel('迭代次数 k', 'FontSize', 12); %[output:825ceb72]
ylabel('理论解 ω(k)', 'FontSize', 12); %[output:825ceb72]
title('理论解析解：ω(k) = 1 - (1-α)^k', 'FontSize', 14); %[output:825ceb72]
grid on; %[output:825ceb72]
xlim([0, N-1]); %[output:825ceb72]
ylim([-0.5, 2.5]); %[output:825ceb72]

%% 3. 收敛时间分析
fprintf('=== 阶跃响应收敛时间分析 ===\n'); %[output:1cba0cc8]
fprintf('理论解: ω(k) = 1 - (1-α)^k\n'); %[output:8b03d096]
fprintf('到达95%%稳态值所需时间: k = log(0.05)/log(|1-α|)\n\n'); %[output:401293a5]

fprintf('α值\t稳定性\t收敛模式\t95%%时间\t振荡频率\n'); %[output:80f739e3]
fprintf('------------------------------------------------------\n'); %[output:2ea2258c]

for i = 1:length(alpha_values) %[output:group:8a9f315b]
    alpha = alpha_values(i);
    
    % 稳定性判断
    if abs(1-alpha) < 1
        stability = '稳定';
    else
        stability = '不稳定';
    end
    
    % 收敛模式
    if alpha < 1
        mode = '单调上升';
    elseif alpha == 1
        mode = '一步到达';
    elseif alpha < 2
        mode = '振荡收敛';
    else
        mode = '发散振荡';
    end
    
    % 计算到达95%稳态值的时间（如果稳定）
    if abs(1-alpha) < 1 && alpha ~= 1
        k_95 = ceil(log(0.05)/log(abs(1-alpha)));
    elseif alpha == 1
        k_95 = 1;  % 一步到达
    else
        k_95 = Inf;  % 不稳定
    end
    
    % 计算振荡频率（如果振荡）
    if alpha > 1 && alpha < 2
        freq = acos(1 - alpha) / pi;  % 归一化频率（×π）
        fprintf('%.1f\t%s\t%s\t%d\t%.2fπ\n', ... %[output:7b4c0202]
                alpha, stability, mode, k_95, freq); %[output:7b4c0202]
    else
        fprintf('%.1f\t%s\t%s\t%d\t-\n', alpha, stability, mode, k_95); %[output:7414a672] %[output:470b1ba0]
    end
end %[output:group:8a9f315b]

